include<iostream>
using namespace std;
int main()
{
	cout << "Enter the time in 24 hour pattern to see whether it Morning, Afternoon, Evening or Night\n";
	double time;
	cin >> time;
	if (time < 12)
	{
		cout << "Good Morning";
	}
	else if (time >= 12 && time < 18)
	{
		cout << "Good Afternoon";
	}
	else if (time >= 18 && time < 21)
	{
		cout << "Good Evening";
	}
	else if (time >= 21 && time < 24)
	{
		cout << "Good Night";
	}
	else
	{
		cout << "Incorrect input";
	}
}
